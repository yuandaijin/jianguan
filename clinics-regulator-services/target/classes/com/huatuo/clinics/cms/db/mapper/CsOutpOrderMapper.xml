  <!-- 处方数量(药品种类, 药品分类, 给药途径) -->
  <select id="countAntibiotic" parameterType="map" resultType="int">
  	select count(distinct coo.ord_id)
	from (
		  select ord_id 
		  from cs_outp_order
		  where 1=1
		  <if test="dateTime != null">
			  and visit_date like #{dateTime} "%"
		  </if>
		  and org_id in 
		  <foreach collection="orgIds" index="index" item="item" open="(" separator="," close=")">
		         #{item}
		  </foreach>
			) coo
	left join cs_outp_order_dtl cood
	on(coo.ord_id = cood.ord_id)
	left join ta_pha_drug_info tpdi
	on(cood.ord_item_id = tpdi.pha_drug_id)
	where 1=1
	<if test="drugType != null">
		and tpdi.drug_type = #{drugType}
	</if>
	<if test="drugSubType != null">
		and tpdi.drug_sub_type = #{drugSubType}
	</if>
	<if test="defRoute != null">
		and tpdi.def_route = #{defRoute}
	</if>
  </select>
  
  
  <select id="queryOrdId" parameterType="java.util.Map" resultMap="BaseResultMap" >
  select <include refid="Base_Column_List" />
    from cs_outp_order a where 
  	a.ord_id NOT IN(
				select b.vist_id
				from cms_jg_ordercheck_dtl b
				left join cms_jg_ordercheck c
				ON(b.order_id=c.check_id)
				where  b.org_id IN
					<foreach item="item" index="index" collection="strList" open="(" separator="," close=")">  
					  #{item}  
					 </foreach>
					 <if test="startDate != null and endDate != null">
					 	and b.order_date &gt;=#{startDate,jdbcType=OTHER}  and b.order_date &lt;=#{endDate,jdbcType=OTHER}
					 </if>
					  <if test="userId != null and userId != ''">
					 	 and c.user_id=#{userId,jdbcType=OTHER}
					 </if>
				    )
		 and a.org_id IN
		 <foreach item="item" index="index" collection="strList" open="(" separator="," close=")">  
				 #{item}  
		 </foreach>
		 <if test="startDate != null and endDate != null">
				and a.visit_date &gt;=#{startDate,jdbcType=OTHER} and a.visit_date &lt;=#{endDate,jdbcType=OTHER}
		 </if>
		 <if test="orderType != null and orderType !=''">
		 	and a.drug_type=#{orderType,jdbcType=OTHER} 
		 </if>
		 and a.valid_flg=#{validFlg,jdbcType=OTHER}
  </select>
  

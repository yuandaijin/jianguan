	where org_id = #{orgId}
  	order by visit_date desc
  	limit 1
  </select>
  
  <!--   多张表关联查询 -->
  <select id="selectRescription" parameterType="com.huatuo.clinics.cms.bean.CsOutpVisitDTO" resultType="com.huatuo.clinics.cms.db.bean.CsOutpVisit">
  	SELECT  s.visti_id vistiId, s.patient_id patientid, s.org_id orgId, s.visti_no vistiNo,
  	        s.visit_date visitdate, s.ord_dep_id ordDepId, s.doctor_id doctorId, s.age age,
  	        s.visit_status visitStatus,s.visit_type visitType,r.patient_name patientName
  	    FROM cs_outp_visit s
		LEFT JOIN rs_empi_patient_info r
		ON(s.patient_id = r.patient_id)
		LEFT JOIN cs_outp_visit_dx c
		ON(s.visti_id =c.visti_id )
		LEFT JOIN cs_outp_order d
		ON(d.visti_id=s.visti_id)
		where 1=1
			<if test="patientName != null and patientName != '' ">
				and r.patient_name LIKE concat('%',#{patientName},'%')
			</if>
			<if test="teleNum !=null and teleNum !=''">
				and r.tele_num LIKE CONCAT('%',#{teleNum},'%')
			</if>
			<if test="beginDate != null and beginDate != ''">
				and s.visit_date &gt;  #{beginDate,jdbcType=CHAR}
			</if>
			<if test="endDate != null and endDate != '' ">
			    and s.visit_date &lt;= DATE_ADD(#{endDate,jdbcType=CHAR},INTERVAL 1 DAY)
			</if>
			<if test="orgId != null and orgId != '' ">
				and	s.org_id = #{orgId,jdbcType=VARCHAR}
			</if>
			<if test="cardId!=null and cardId!=''">
				and r.card_id like concat('%',#{cardId},'%')
			</if>
				AND  c.icd_name_new IS NOT NULL
				AND d.valid_flg =1
			 GROUP BY s.visti_id
			 ORDER BY s.visit_date DESC
  </select>
  	<!-- 收费查询左侧菜单：未收费且处方单有效 -->
     <select id="queryVisits" parameterType="java.util.Map" resultMap="BaseResultMap">
    select v.* 
		FROM cs_outp_visit v
		join cs_outp_order o on (v.visti_id = o.visti_id)
		join cs_outp_order_dtl d on(o.ord_id = d.ord_id)
		where v.org_id = #{orgId} 
		AND o.valid_flg = '1'
		AND TO_DAYS(v.visit_date) = TO_DAYS(NOW()) 
		AND o.is_charge = "0" 
		GROUP BY v.visti_id DESC
  </select>
  
  <select id="queryAll" parameterType="java.util.Map" resultMap="BaseResultMap">
  	select *
  	from cs_outp_visit
  	where org_id = #{orgId}
  	and visit_date &gt;= #{queryDate1} 
  	and visit_date &lt; #{queryDate2}
  </select>
  
  <!-- 门诊总量 -->
  <select id="countVisit" parameterType="map" resultType="int">
  	select count(visti_id)
  	from cs_outp_visit
  	where 1=1
    <if test="dateTime != null">
		and	visit_date like #{dateTime} "%"
	</if>
    <if test="visitType != null">
		and	visit_type = #{visitType}
	</if>
	<if test="orgIds != null">
	  	and org_id in
	  	<foreach collection="orgIds" index="index" item="item" open="(" separator="," close=")">
	            #{item}
	    </foreach>
    </if>
    <!-- 个体诊所中、西医门诊量总计 -->
    <if test="orgIds2 != null">
		and org_id in
		<foreach collection="orgIds2" index="index" item="item" open="(" separator="," close=")">
	            #{item}
	    </foreach>
    </if>
  </select>
  
  
  <select id="getByVisitDateAndVisitNo" parameterType="java.util.Map" resultMap="BaseResultMap">
  	select *
  	from cs_outp_visit
  	where org_id = #{orgId}
  	and visit_date &gt;= #{visitDate1} 
  	and visit_date &lt;= #{visitDate2}
  	and visti_no = #{visitNo}
  </select>
  
  <!-- 查询出就诊id集合 -->
  <select id="listVisitId" parameterType="map" resultType="string">
  	select visti_id
	from cs_outp_visit
	where 1=1 
	<if test="dateTime != null">
		and	visit_date like #{dateTime} "%"
	</if>
	<if test="orgIds != null">
		and  
		<foreach collection="orgIds" index="index" item="item" open="(" separator="or" close=")">
	          org_id = #{item}
	    </foreach>
	</if>
  </select>
  
  
  <!-- 查询出就诊id集合 -->
  <select id="countIcdBySexOrAge" parameterType="map" resultType="int">
  	<if test="icdCode != null">
	  	select count(covd.visti_id)
		from (
				select visti_id 
				from cs_outp_visit_dx
				where icd_code = #{icdCode}
			  	<if test="visitIds != null">
					and  
					<foreach collection="visitIds" index="index" item="item" open="(" separator="or" close=")">
				          visti_id = #{item}
				    </foreach>
				</if>
				) covd
		left join cs_outp_visit cov on(covd.visti_id = cov.visti_id)
		left join rs_empi_patient_info repi on(cov.patient_id = repi.patient_id)
		where 1=1
		<if test="sex != null">
			and repi.sex = #{sex}
		</if>
		<if test="startYear != null">
			and repi.birthday > #{startYear}
		</if>
		<if test="endYear != null">
			and  #{endYear} > repi.birthday
		</if>
  	</if>
  </select>
  
  
